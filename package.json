{
  "name": "Communicode",
  "version": "1.0.0",
  "description": "A boilerplate for playing around with react, redux and react-router with the help of webpack.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "node server.dev.js",
    "start:prod": "node server.bundle.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.production.config.js --progress --profile --colors",
    "build:server": "cross-env NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.server.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "bundle": "./node_modules/webpack/bin/webpack.js --config webpack.production.config.js",
    "bundle-dev": "./node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "deploy": "npm run build && cd dist && gcloud app deploy app.yaml --no-promote",
    "deploy-production": "npm run build && cd dist && gcloud app deploy app.yaml --promote"
  },
  "keywords": [
    "es6",
    "react",
    "redux"
  ],
  "author": "Communicode LLC",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "critical": "^0.8.4",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-https-redirect": "^1.0.0",
    "if-env": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-ga": "^2.2.0",
    "react-native-listener": "^1.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-title-component": "^1.0.1",
    "redux": "^3.5.2",
    "redux-saga": "^0.15.3",
    "sitemap": "^1.12.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.23.1",
    "ejs-loader": "^0.3.0",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.16.1",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router-path-extractor-webpack-plugin": "^0.1.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sitemap-webpack-plugin": "^0.3.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^1.14.1"
  }
}
