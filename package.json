{
  "name": "Communicode",
  "version": "1.0.0",
  "description": "A boilerplate for playing around with react, redux and react-router with the help of webpack.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "rimraf dist && ./node_modules/webpack/bin/webpack.js --config ./webpack.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "bundle": "./node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "prestart": "npm run bundle"
  },
  "keywords": [
    "es6",
    "react",
    "redux"
  ],
  "author": "Jean-Pierre Sierens",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-native-listener": "^1.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
